{
  "name": "webtoolrunscripts",
  "version": "1.0.0",
  "description": "WebApp for running cmd/bat scripts. MERN stack.",
  "private": true,
  "author": "Galyuk Viktor",
  "lincese": "MIT",
  "main": "server.js",
  "scripts": {
    "client-install": "npm install --prefix client",
    "start": "node ./bin/www",
    "server": "nodemon server.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm start server\" \"npm run client\""
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "concurrently": "^5.3.0",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "mongoose": "^5.10.13",
    "morgan": "~1.9.1"
  },
  "devDependencies": {
    "nodemon": "^2.0.6"
  }
}
