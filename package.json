{
  "name": "webtoolrunscripts",
  "version": "1.0.0",
  "description": "WebApp for running cmd/bat scripts. MERN stack.",
  "private": true,
  "author": "Galyuk Viktor",
  "lincese": "MIT",
  "main": "server.js",
  "scripts": {
    "client-install": "npm install --prefix client",
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "config": "^3.3.2",
    "express": "~4.16.1",
    "express-fileupload": "^1.2.0",
    "mongoose": "^5.10.13",
    "multer": "^1.4.2"
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "nodemon": "^2.0.6"
  }
}
